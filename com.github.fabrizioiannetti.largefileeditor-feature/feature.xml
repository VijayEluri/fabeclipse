<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.github.fabrizioiannetti.largefileeditor_feature"
      label="Large File Editor"
      version="1.0.0.qualifier"
      provider-name="Fabrizio Iannetti">

   <description url="https://github.com/fabrizioiannetti/fabeclipse">
      Simple editor to view very long files (without loading the content into memory)
   </description>

   <copyright url="http://fabrizioiannetti.github.io/fabeclipse/">
      Copyright Fabrizio Iannetti.
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      This program and the accompanying materials
are made available under the terms of the Eclipse Public License
v1.0
which accompanies this distribution, and is available at

http://www.eclipse.org/org/documents/epl-v10.html
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.workbench"/>
      <import plugin="org.eclipse.ui.ide" version="3.9.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.osgi" version="3.9.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface.text" version="3.8.101" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.workbench.texteditor" version="3.8.101" match="greaterOrEqual"/>
      <import plugin="com.github.fabeclipse.textedgrep" version="1.2.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.editors"/>
   </requires>

   <plugin
         id="com.github.fabrizioiannetti.largefileeditor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
